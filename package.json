{
  "name": "gafi-dashboard",
  "version": "monthly-2022-01",
  "private": true,
  "author": "Parity Technologies <admin@parity.io>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/cryptoviet/gafi-app/issues"
  },
  "keywords": [
    "substrate",
    "substrate-ui",
    "polkadot-js"
  ],
  "scripts": {
    "start": "react-app-rewired start",
    "start:prod": "env-cmd -f .env.production react-app-rewired start",
    "build": "env-cmd -f .env.production react-app-rewired build",
    "typegen": "yarn generate:defs && yarn generate:meta",
    "mainnet:typegen": "yarn mainnet:generate:defs && yarn mainnet:generate:meta",
    "generate:defs": "ts-node --skip-project node_modules/.bin/polkadot-types-from-defs --package gafi-dashboard/interfaces --input ./src/interfaces --endpoint ./edgeware.json",
    "generate:meta": "ts-node --skip-project node_modules/.bin/polkadot-types-from-chain --package gafi-dashboard/interfaces --endpoint ./edgeware.json --output ./src/interfaces",
    "generate:graphql": "graphql-codegen",
    "test": "CI=true react-app-rewired test",
    "eject": "react-scripts eject",
    "eslint:check": "eslint --ext .tsx,.ts src/",
    "eslint-fix": "eslint --cache --fix --max-warnings=96 --ext .tsx,.ts src/",
    "prettier:check": "prettier -c src/*",
    "prettier:write": "prettier -w src/*",
    "postinstall": "husky install",
    "hooks:pre-commit": "lint-staged",
    "deploy": "gh-pages -d build -m '[ci skip] Updates'",
    "deploy:mainnet": "yarn build && firebase use gafi-c2fcf && firebase deploy"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.7",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@ethersproject/bignumber": "^5.6.0",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-operations": "^2.4.0",
    "@graphql-codegen/typescript-react-query": "^3.5.12",
    "@hookform/error-message": "^2.0.0",
    "@mdi/js": "^6.6.96",
    "@mdi/react": "^1.5.0",
    "@polkadot/api": "^8.1.1",
    "@polkadot/extension-dapp": "^0.42.7",
    "@polkadot/keyring": "^8.4.1",
    "@polkadot/networks": "^8.4.1",
    "@polkadot/types": "^8.1.1",
    "@polkadot/ui-keyring": "^1.1.1",
    "@polkadot/ui-settings": "^1.1.1",
    "@polkadot/util": "^9.0.1",
    "@polkadot/util-crypto": "^8.4.1",
    "ethers": "^5.6.8",
    "framer-motion": "^6",
    "graphql": "^16.5.0",
    "graphql-request": "^4.2.0",
    "i18next": "^21.8.0",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.0",
    "lodash": "^4.17.21",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.2",
    "react-dropzone": "^12.0.4",
    "react-hook-form": "^7.30.0",
    "react-i18next": "^11.16.9",
    "react-query": "^3.34.19",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.0",
    "stream-browserify": "^3.0.0",
    "use-query-params": "^1.2.3",
    "use-wallet": "^0.13.4",
    "web3": "^1.7.1"
  },
  "devDependencies": {
    "@polkadot/typegen": "^8.1.1",
    "@testing-library/react": "release-12.x",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^28.1.4",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "gh-pages": "^3.2.3",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "2.5.1",
    "react-app-rewired": "^2.2.1",
    "semistandard": "^16.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "eslint-plugin-jsx-a11y": "6.5.1"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.(t|j)s?(x)": "yarn eslint-fix"
  },
  "packageManager": "yarn@3.1.1"
}
